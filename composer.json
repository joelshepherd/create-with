{
    "name": "joelshepherd/create-with",
    "description": "Easily add common unique identity fields to Laravel models.",
    "type": "library",
    "keywords": ["laravel", "eloquent", "uuid", "slug"],
    "license": "MIT",
    "authors": [
        {
            "name": "Joel Shepherd",
            "email": "joel@swivid.ink"
        }
    ],
    "autoload": {
        "psr-4": {
            "JoelShepherd\\CreateWith\\": "src/"
        }
    },
    "autoload-dev": {
